<div class="my-5">
    <h1 class="py-3">Second step</h1>

    <form action="{{ path('user_registration_registration') }}" method="post" enctype="multipart/form-data">
        <div class="form-group">
            {{ form_label(form.firstName, 'First name', { 'label_attr': {'for': 'firstName'} }) }}
            {#<input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter email">#}
            {{ form_widget(form.firstName, { 'attr': {'class': 'form-control'}, 'id': 'firstName' }) }}
        </div>
        <div class="form-group">
            {{ form_label(form.secondName, 'Second name', { 'label_attr': {'for': 'secondName'} }) }}
            {#<input type="password" class="form-control" id="exampleInputPassword1" placeholder="Password">#}
            {{ form_widget(form.secondName, { 'attr': {'class': 'form-control'}, 'id': 'secondName' }) }}
        </div>
        <div class="form-group">
            {{ form_label(form.country, 'Country', { 'label_attr': {'for': 'country'} }) }}
            {#<input type="password" class="form-control" id="exampleInputPassword1" placeholder="Password">#}
            {{ form_widget(form.country, { 'attr': {'class': 'form-control'}, 'id': 'country' }) }}
        </div>
        {{ form_widget(form._token) }}

        <button type="submit" class="btn btn-primary">Submit</button>
    </form>
</div>